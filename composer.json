{
  "type": "library",
  "name": "autoframe/core",
  "description": "Autoframe Framework CORE",
  "license": "MIT",
  "homepage": "https://github.com/autoframe",
  "require": {
    "php": ">=7.4",
    "composer/class-map-generator": ">=1.0",
    "ext-json": "*",
    "ext-pdo": "*"
  },
  "version": "dev-1.0.0",
  "authors": [
    {
      "name": "Nistor Alexandru Marius",
      "homepage": "https://autoframe.ro",
      "role": "Developer"
    }
  ],
  "keywords": [],
  "autoload": {
    "psr-4": {
      "Autoframe\\Core\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Unit\\": "Tests/Unit",
      "Unit\\*\\": "Tests/Unit/*/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6 || ^10"
  },
  "suggest": {
    "ext-sockets": "Socket connection client - server",
    "ext-zlib": "Sock connection compression",
    "ext-memcached": "Memcached extension",
    "ext-apcu": "APC cache extension",
    "predis/predis": "^2.0 Redis support - dev bugs",
    "ext-simplexml": "SimpleXMLElement parser",
    "ext-ftp": "*"
  }
}